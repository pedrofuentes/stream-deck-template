{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
  "Actions": [
    {
      "Icon": "imgs/actions/__ACTION_NAME__/icon",
      "Name": "__ACTION_DISPLAY__",
      "States": [
        {
          "Image": "imgs/actions/__ACTION_NAME__/key",
          "ShowTitle": false
        }
      ],
      "Tooltip": "__ACTION_DISPLAY__ description",
      "UUID": "__PLUGIN_ID__.__ACTION_NAME__",
      "PropertyInspectorPath": "ui/__ACTION_NAME__.html"
    }
  ],
  "Author": "__AUTHOR_SHORT__",
  "Category": "__PLUGIN_NAME__",
  "CategoryIcon": "imgs/plugin-icon",
  "CodePath": "bin/plugin.js",
  "Description": "__PLUGIN_DESCRIPTION__",
  "Icon": "imgs/plugin-icon",
  "Name": "__PLUGIN_NAME__",
  "Nodejs": {
    "Version": "20",
    "Debug": "enabled"
  },
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "13"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.9"
  },
  "URL": "__REPO_URL__",
  "UUID": "__PLUGIN_ID__",
  "Version": "1.0.0.0"
}
